{
  "name": "@clawswap/sdk",
  "version": "0.2.0",
  "description": "Gasless cross-chain token swaps for AI agents via x402 micropayments. Solana â†” Base bidirectional. No API key, no gas management.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run --exclude tests/integration.test.ts",
    "test:integration": "vitest run tests/integration.test.ts",
    "test:all": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm clean && pnpm build && pnpm test"
  },
  "keywords": [
    "x402",
    "solana",
    "base",
    "cross-chain",
    "swap",
    "ai-agents",
    "usdc",
    "defi",
    "bridge",
    "elizaos",
    "eliza",
    "elizaos-plugins",
    "moltbook",
    "typescript",
    "sdk"
  ],
  "author": "ClawSwap",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/WarTech9/clawswap-sdk.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://clawswap.dev",
  "bugs": {
    "url": "https://github.com/WarTech9/clawswap-sdk/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "@x402/fetch": "^2.3.0"
  },
  "peerDependenciesMeta": {
    "@x402/fetch": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
