{
  "name": "@clawswap/goat-plugin",
  "version": "0.1.0",
  "description": "GOAT SDK plugin for ClawSwap - Gasless Solana â†’ Base cross-chain swaps via x402. $0.50 flat fee, no API key, no gas.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm clean && pnpm build && pnpm test"
  },
  "keywords": [
    "x402",
    "solana",
    "base",
    "cross-chain",
    "swap",
    "ai-agents",
    "goat-sdk",
    "goat",
    "usdc",
    "defi",
    "bridge",
    "typescript",
    "plugin"
  ],
  "author": "ClawSwap",
  "license": "MIT",
  "homepage": "https://clawswap.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/WarTech9/clawswap-sdk.git",
    "directory": "packages/goat-plugin"
  },
  "bugs": {
    "url": "https://github.com/WarTech9/clawswap-sdk/issues"
  },
  "dependencies": {
    "@clawswap/sdk": "workspace:*",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "@goat-sdk/core": "^0.3.0",
    "@x402/fetch": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
