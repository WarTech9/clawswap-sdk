# Solana private key for Solana payments (base58 encoded)
# Required when using --payment-chain solana (default)
SOLANA_PRIVATE_KEY=

# EVM private key for Base payments (hex format with 0x prefix)
# Required when using --payment-chain base
EVM_PRIVATE_KEY=0x

# Optional: Override API URL
CLAWSWAP_API_URL=https://api.clawswap.dev

# Optional: Test mode
# mock = no API calls, dry-run = API calls but no swap, full = complete flow
TEST_MODE=dry-run
